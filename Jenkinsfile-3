pipeline {
  agent any
  stages {
     stage("Verify docker tooling in Agent") {
        steps {
            sh '''
                docker info
                docker version
                docker compose version
            '''
        }
     }
  }
  agent {
     label 'agent-linux-001'
  }
  stages {
     stage("Verify agent linux") {
        script {
            echo 'Some file permissions could not be updated.'
        }
     }
  }
}


